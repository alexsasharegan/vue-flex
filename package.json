{
  "name": "vue-flex",
  "version": "2.0.5",
  "description": "A reusable flexbox component using functional css.",
  "main": "dist/vue-flex.js",
  "module": "dist/vue-flex.esm.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "rollup -c ./rollup.config.js",
